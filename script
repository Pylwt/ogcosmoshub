local function resetUI()
    Window:Destroy()
    Window = Library.CreateLib("Welcome To Cosmos Hub - By Cosmos#5091", "DarkTheme")
    AdminTab = Window:NewTab("Admin Commands")
    GamesTab = Window:NewTab("Games")
    MoreTab = Window:NewTab("More")
    InfoTab = Window:NewTab("Info")
    -- Recreate all sections and buttons here
    -- Admin Commands
    AdminSection = AdminTab:NewSection("Commands")
    -- ... (Copy code from above to recreate Admin Commands) ...
    AdminSection:NewToggle("Fly", "Toggle flying ability", function(state)
        if state then
            loadstring(game:HttpGet("https://pastebin.com/raw/YwwvHbTq"))()
        else
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid.PlatformStand = false
            end
        end
    end)
    AdminSection:NewToggle("ESP", "Toggle ESP for players", function(state)
        if state then
            loadstring(game:HttpGet("https://pastebin.com/raw/FsJak6AT"))()
        else
            for _, v in pairs(game.CoreGui:GetChildren()) do
                if v.Name == "ESP" then
                    v:Destroy()
                end
            end
        end
    end)
    AdminSection:NewToggle("Noclip", "Walk through walls", function(state)
        if state then
            loadstring(game:HttpGet("https://pastebin.com/raw/MHE8bZVq"))()
        else
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
            end
        end
    end)
    AdminSection:NewToggle("Aimbot", "Enable aimbot", function(state)
        if state then
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Exunys/Aimbot-Script/main/Aimbot%20Script.lua", true))()
        else
            getgenv().AimbotSettings = nil
        end
    end)
    AdminSection:NewButton(";jump", "Makes you jump", function() jump(LocalPlayer) end)
    AdminSection:NewButton(";kill", "Kills the player", function() kill(LocalPlayer) end)
    AdminSection:NewButton(";loopkill", "Kills a player over and over", function() loopKill(LocalPlayer) end)
    AdminSection:NewButton(";sparkles", "Adds sparkles effect", function() sparkles(LocalPlayer) end)
    AdminSection:NewButton(";unsparkles", "Removes sparkles effect", function() unsparkles(LocalPlayer) end)
    AdminSection:NewButton(";ff", "Creates a force field", function() forceField(LocalPlayer) end)
