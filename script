        setclipboard(coordsLabel.Text:sub(14)) -- Remove "Coordinates: "
    end)
    InfoSection:NewLabel("Game ID: " .. tostring(game.GameId))
    InfoSection:NewLabel("Current Time: " .. os.date("%X"))
    InfoSection:NewLabel("Current Date: " .. os.date("%x"))
    InfoSection:NewLabel("Total Players: " .. #Players:GetPlayers())
    InfoSection:NewLabel("Your Username: " .. LocalPlayer.Name)
    InfoSection:NewLabel("All Players: " .. table.concat(Players:GetPlayers():map(function(p) return p.Name end), ", "))
    InfoSection:NewLabel("Credits: Cosmos#5091")
    InfoSection:NewLabel("Misc: This UI is designed for admin purposes.")
    -- Live Coordinates Update
    coroutine.wrap(function()
        while true do
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local pos = LocalPlayer.Character.HumanoidRootPart.Position
                coordsLabel.Text = "Coordinates: " .. string.format("%.2f, %.2f, %.2f", pos.X, pos.Y, pos.Z)
            end
            wait(1)
        end
    end)()
end

-- Reset UI on Character Death/Reset
LocalPlayer.CharacterAdded:Connect(resetUI)
            pcall(function()
