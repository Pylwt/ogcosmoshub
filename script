    {"Mini Head Toggle", "https://pastebin.com/raw/STU404", function() miniHead(LocalPlayer) end, function() normalHead(LocalPlayer) end},
    {"Big Head Toggle", "https://pastebin.com/raw/VWX505", function() bigHead(LocalPlayer) end, function() normalHead(LocalPlayer) end},
