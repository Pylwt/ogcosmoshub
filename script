-- OG Cosmos Hub
-- Made by Cosmos#5091

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Admins = {"AdminUsername1", "AdminUsername2"} -- Add admin usernames here

-- Load OG Cosmos Hub UI Library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Welcome To Cosmos Hub - By Cosmos#5091", "DarkTheme")

-- Function to check if player is an admin
local function isAdmin(player)
    for _, admin in pairs(Admins) do
        if player.Name == admin then
            return true
        end
    end
    return false
end

-- Create Tabs
local AdminTab = Window:NewTab("Admin Commands")
local GamesTab = Window:NewTab("Games")
local MoreTab = Window:NewTab("More")
local InfoTab = Window:NewTab("Info")

-- Admin Commands Section
local AdminSection = AdminTab:NewSection("Commands")

-- Toggle Functions
local toggles = {}
local function toggleFeature(name, enableFunc, disableFunc)
    if toggles[name] then
        disableFunc()
        toggles[name] = false
    else
        enableFunc()
        toggles[name] = true
    end
end

-- Fly
AdminSection:NewToggle("Fly", "Toggle flying ability", function(state)
    if state then
        loadstring(game:HttpGet("https://pastebin.com/raw/YwwvHbTq"))()
    else
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.PlatformStand = false
        end
    end
end)

-- ESP
AdminSection:NewToggle("ESP", "Toggle ESP for players", function(state)
    if state then
        loadstring(game:HttpGet("https://pastebin.com/raw/FsJak6AT"))()
    else
        for _, v in pairs(game.CoreGui:GetChildren()) do
            if v.Name == "ESP" then
                v:Destroy()
            end
        end
    end
end)

-- Noclip
AdminSection:NewToggle("Noclip", "Walk through walls", function(state)
    if state then
        loadstring(game:HttpGet("https://pastebin.com/raw/MHE8bZVq"))()
    else
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
        end
    end
end)

-- Aimbot
AdminSection:NewToggle("Aimbot", "Enable aimbot", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Exunys/Aimbot-Script/main/Aimbot%20Script.lua", true))()
    else
        getgenv().AimbotSettings = nil
    end
end)

-- Command Functions
local function jump(player)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.Jump = true
    end
end

local function kill(player)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.Health = 0
    end
end

local function loopKill(player)
    local loop = toggles["LoopKill_" .. player.Name]
    if loop then
        toggles["LoopKill_" .. player.Name] = false
    else
        toggles["LoopKill_" .. player.Name] = true
        coroutine.wrap(function()
            while toggles["LoopKill_" .. player.Name] do
                if player.Character and player.Character:FindFirstChild("Humanoid") then
                    player.Character.Humanoid.Health = 0
                end
                wait(0.5)
            end
        end)()
    end
end

local function sparkles(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local sparklesEffect = Instance.new("ParticleEmitter")
        sparklesEffect.Texture = "rbxassetid://102604810"
        sparklesEffect.Parent = player.Character.HumanoidRootPart
        sparklesEffect.Name = "SparklesEffect"
    end
end

local function unsparkles(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local sparklesEffect = player.Character.HumanoidRootPart:FindFirstChild("SparklesEffect")
        if sparklesEffect then
            sparklesEffect:Destroy()
        end
    end
end

local function forceField(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local forceFieldPart = Instance.new("Part")
        forceFieldPart.Shape = Enum.PartType.Ball
        forceFieldPart.Size = Vector3.new(6, 6, 6)
        forceFieldPart.CFrame = player.Character.HumanoidRootPart.CFrame
        forceFieldPart.Anchored = true
        forceFieldPart.CanCollide = false
        forceFieldPart.Transparency = 0.5
        forceFieldPart.BrickColor = BrickColor.new("Cyan")
        forceFieldPart.Parent = player.Character
        forceFieldPart.Name = "ForceField"
    end
end

local function unforceField(player)
    if player.Character and player.Character:FindFirstChild("ForceField") then
        player.Character.ForceField:Destroy()
    end
end

local function fire(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local fireEffect = Instance.new("Fire")
        fireEffect.Parent = player.Character.HumanoidRootPart
        fireEffect.Name = "FireEffect"
    end
end

local function unfire(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local fireEffect = player.Character.HumanoidRootPart:FindFirstChild("FireEffect")
        if fireEffect then
            fireEffect:Destroy()
        end
    end
end

local function smoke(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local smokeEffect = Instance.new("Smoke")
        smokeEffect.Parent = player.Character.HumanoidRootPart
        smokeEffect.Name = "SmokeEffect"
    end
end

local function unsmoke(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local smokeEffect = player.Character.HumanoidRootPart:FindFirstChild("SmokeEffect")
        if smokeEffect then
            smokeEffect:Destroy()
        end
    end
end

local function miniHead(player)
    if player.Character and player.Character:FindFirstChild("Head") then
        player.Character.Head.Size = Vector3.new(1, 1, 1)
    end
end

local function bigHead(player)
    if player.Character and player.Character:FindFirstChild("Head") then
        player.Character.Head.Size = Vector3.new(3, 3, 3)
    end
end

local function normalHead(player)
    if player.Character and player.Character:FindFirstChild("Head") then
        player.Character.Head.Size = Vector3.new(2, 2, 2)
    end
end

local function trip(player)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.WalkSpeed = 0
        wait(3)
        player.Character.Humanoid.WalkSpeed = 16
    end
end

local function sit(player)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Seated)
    end
end

local function invisible(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.Transparency = 1
        for _, part in pairs(player.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Transparency = 1
            end
        end
    end
end

local function visible(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.Transparency = 0
        for _, part in pairs(player.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Transparency = 0
            end
        end
    end
end

local function freeze(player)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.WalkSpeed = 0
        player.Character.Humanoid.JumpPower = 0
    end
end

local function unfreeze(player)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.WalkSpeed = 16
        player.Character.Humanoid.JumpPower = 50
    end
end

local function explode(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local explosion = Instance.new("Explosion")
        explosion.Position = player.Character.HumanoidRootPart.Position
        explosion.Parent = game.Workspace
    end
end

local function merge(player1, player2)
    if player1.Character and player1.Character:FindFirstChild("HumanoidRootPart") and player2.Character and player2.Character:FindFirstChild("HumanoidRootPart") then
        player2.Character.HumanoidRootPart.CFrame = player1.Character.HumanoidRootPart.CFrame
    end
end

local function control(player1, player2)
    -- Complex control logic here (requires more advanced scripting)
end

local function giveTools(player)
    if player.Backpack then
        local starterPack = game:GetService("StarterPack"):GetChildren()
        for _, tool in pairs(starterPack) do
            tool:Clone().Parent = player.Backpack
        end
    end
end

local function removeTools(player)
    if player.Backpack then
        for _, tool in pairs(player.Backpack:GetChildren()) do
            if tool:IsA("Tool") then
                tool:Destroy()
            end
        end
    end
end

local function jail(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local jailPart = Instance.new("Part")
        jailPart.Size = Vector3.new(8, 8, 8)
        jailPart.CFrame = player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 4, 0)
        jailPart.Anchored = true
        jailPart.CanCollide = true
        jailPart.Transparency = 0.5
        jailPart.BrickColor = BrickColor.new("Gray")
        jailPart.Parent = game.Workspace
        jailPart.Name = "Jail" .. player.Name
    end
end

local function unjail(player)
    if game.Workspace:FindFirstChild("Jail" .. player.Name) then
        game.Workspace["Jail" .. player.Name]:Destroy()
    end
end

local function godMode(player)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.MaxHealth = math.huge
        player.Character.Humanoid.Health = math.huge
    end
end

local function ungodMode(player)
    if player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.MaxHealth = 100
        player.Character.Humanoid.Health = 100
    end
end

local function kick(player)
    player:Kick("You have been kicked by an admin.")
end

local function zombify(player)
    -- Complex zombie logic here
end

local function admin(player)
    table.insert(Admins, player.Name)
end

local function unadmin(player)
    for i, v in ipairs(Admins) do
        if v == player.Name then
            table.remove(Admins, i)
            break
        end
    end
end

-- Command Buttons
AdminSection:NewButton(";jump", "Makes you jump", function() jump(LocalPlayer) end)
AdminSection:NewButton(";kill", "Kills the player", function() kill(LocalPlayer) end)
AdminSection:NewButton(";loopkill", "Kills a player over and over", function() loopKill(LocalPlayer) end)
AdminSection:NewButton(";sparkles", "Adds sparkles effect", function() sparkles(LocalPlayer) end)
AdminSection:NewButton(";unsparkles", "Removes sparkles effect", function() unsparkles(LocalPlayer) end)
AdminSection:NewButton(";ff", "Creates a force field", function() forceField(LocalPlayer) end)
AdminSection:NewButton(";unff", "Removes the force field", function() unforceField(LocalPlayer) end)
AdminSection:NewButton(";fire", "Sets player on fire", function() fire(LocalPlayer) end)
AdminSection:NewButton(";unfire", "Stops the fire", function() unfire(LocalPlayer) end)
AdminSection:NewButton(";smoke", "Adds smoke effect", function() smoke(LocalPlayer) end)
AdminSection:NewButton(";unsmoke", "Removes smoke effect", function() unsmoke(LocalPlayer) end)
AdminSection:NewButton(";minihead", "Makes head smaller", function() miniHead(LocalPlayer) end)
AdminSection:NewButton(";bighead", "Makes head bigger", function() bigHead(LocalPlayer) end)
AdminSection:NewButton(";normalhead", "Resets head size", function() normalHead(LocalPlayer) end)
AdminSection:NewButton(";trip", "Trips the player", function() trip(LocalPlayer) end)
AdminSection:NewButton(";sit", "Forces player to sit", function() sit(LocalPlayer) end)
AdminSection:NewButton(";invisible", "Makes player invisible", function() invisible(LocalPlayer) end)
AdminSection:NewButton(";visible", "Makes player visible", function() visible(LocalPlayer) end)
AdminSection:NewButton(";freeze", "Freezes player", function() freeze(LocalPlayer) end)
AdminSection:NewButton(";explode", "Explodes player", function() explode(LocalPlayer) end)
AdminSection:NewButton(";givetools", "Gives starter tools", function() giveTools(LocalPlayer) end)
AdminSection:NewButton(";removetools", "Removes starter tools", function() removeTools(LocalPlayer) end)
AdminSection:NewButton(";jail", "Jails the player", function() jail(LocalPlayer) end)
AdminSection:NewButton(";unjail", "Unjails the player", function() unjail(LocalPlayer) end)
AdminSection:NewButton(";god mode", "Enables god mode", function() godMode(LocalPlayer) end)
AdminSection:NewButton(";ungod mode", "Disables god mode", function() ungodMode(LocalPlayer) end)
AdminSection:NewButton(";kick", "Kicks the player", function() kick(LocalPlayer) end)
AdminSection:NewButton(";admin", "Gives admin access", function() admin(LocalPlayer) end)
AdminSection:NewButton(";unadmin", "Revokes admin access", function() unadmin(LocalPlayer) end)
AdminSection:NewButton(";die", "Forces character to die", function() kill(LocalPlayer) end)
AdminSection:NewButton(";setspawn", "Sets spawn location", function()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        LocalPlayer.RespawnLocation = Instance.new("SpawnLocation", game.Workspace)
        LocalPlayer.RespawnLocation.Position = LocalPlayer.Character.HumanoidRootPart.Position
    end
end)

